#include "hsp3dish.as"
#pack	"parts.png"
#pack	"chara.png"
#pack	"se_clear.wav"
#pack	"se_death.wav"
#pack	"se_jump2.wav"
#pack	"se_puyo.wav"

	title "Define 1/10"
	
	#define IWINX 640
	#define IWINY 480
	AppTitle="AbellApple 〜「大好物」に導かれし勇者〜"

	sw=ginfo_winx: sh=ginfo_winy	// 実行時の端末解像度x,y
	dm=double(sw)/IWINX				// スケーリング倍率（X基準
	
	dim area_a, 4
	dim area_b, 4
	dim area_c, 4
	
	area_a = 460, 320, 560, 420		//6
	area_b = 40, 140, 140, 420		//5
	area_c = 160, 320, 260, 420		//4

	sx=40:sy=40	; セルの大きさ
	mx=16:my=12	; マップを並べる大きさ
	wx=640:wy=480	; 画面のサイズ

	hitparts=3
	mysizex=64:mysizey=64
	onerror gosub *error

	;	素材の読み込み
	celload "parts.png",2:title "Load PARTS.PNG 2/10"
	celdiv 2,sx,sy
	celload "chara.png",3:title "Load CHARA.PNG 3/10"
	celdiv 3,mysizex,mysizey

	celload "migi.png",4:title "Load MIGI.PNG 4/10"
	celload "hidari.png",5:title "Load HIDARI.PNG 5/10"
	celload "ue.png",6:title "Load UE.PNG 6/10"

	;	音のデータを登録
	mmload "se_clear.wav",0:title "LOAD SE_CLEAR.WAV 7/10"
	mmload "se_death.wav",1:title "LOAD SE_DEATH.WAV 8/10"
	mmload "se_jump2.wav",3:title "LOAD SE_JUMP2.WAV 9/10"
	mmload "se_puyo.wav",4:title "LOAD SE_PUYO.WAV 10/10"
	IsNeedPad=0
	title AppTitle

*gtitle
	redraw 0
	pos 0,0:clrobj
	color 0,128,128:boxf:color 255,255,255
	font msgothic,30,1
	pos 20,40
	mes AppTitle
	font msgothic,18,1
	pos 48,100
	mes "<<ルール>>  \n・腐った林檎を触ったら死んじゃうよ  \n・操作方法を選択しよう(PCなら矢印・ｽﾍﾟｰｽ)   \n・★付きのコースは元ネタシリーズがあるよ！"
	mes "\n操作補助を選択"
	font "",16
	mes "三ボタン式タップの場合、\n機種によっては反応エリアが右下になることがあるので注意！"
	objsize 160,20 //IsNeedPad 0=タップ 1=らら 2=なし
	if IsNeedPad=0{tmp="三ボタンタップ"}else:if IsNeedPad=1{tmp="ジェスジャータップ(重い)"}else{tmp="なし、PC向け"}
	input tmp,300,20
	objsize 50,50
	pos 500,200:button "ﾓｰﾄﾞ変更",*change
	pos 40,320:button "ｵｰﾌﾟﾆﾝｸﾞ",*opening
	pos 100,320:button "1",*type1
	pos 160,320:button "2",*type2
	pos 220,320:button "3★10万",*type3
	pos 280,320:button "4★20万",*type4
	pos 340,320:button "5★30万",*type5
	pos 400,320:button "6★40万",*type6
	pos 460,320:button "7★60万",*type7
	pos 520,320:button "8★70万",*type8
	if Epi=1{pos 40,390:button "ｴﾋﾟﾛｰｸﾞ",*epis}
	redraw 1
	await 1
	goto *gtitle

*change
	IsNeedPad++:if IsNeedPad=3{IsNeedPad=0}
	goto *gtitle
	
