			if s2="O" : a=24
			if s2="P" : a=25
			map(cnt,y)=a		; マップデータを代入する
		loop
		y=y+1
	loop

	;	プレイヤーの初期化
	coin=0		; コインの数

	myflag=0
	myani=0:mydir=0
	myjmp=0:mygrv=0
	px=0:py=vmaxy-sy-mysizey
	font msgothic,16

	mmstop

*main
	;	メインループ
	redraw 0
	gosub *drawbg		; 奥の背景を描画
	gosub *drawmap		; マップを描画
	gosub *mymove		; プレイヤーの移動
	color 255,255,255:pos 0,0:font "",30
	mes "コース "+MapTitle+" [World"+MapScore+"]\n":font "",18
	mes "回収した林檎:"+coin+"/"+coinmax+"\nあと"+str(coinmax-yuuyo-coin)+"個とったらクリア"
	mes "["+viewx+","+viewy+"]"
	IF MapScore=4{
		IF VIEWY>700{colors(0)=50:colors(1)=50:colors(2)=50}else{colors(0)=86:colors(1)=233:colors(2)=233}
		}
	IF MapScore=8{
		IF VIEWX<3000 AND VIEWY>700{colors(0)=163:colors(1)=163:colors(2)=163}else:IF VIEWX>2999 AND VIEWY>700{colors(0)=163:colors(1)=237:colors(2)=150}else{colors(0)=86:colors(1)=233:colors(2)=233}
		}

	if IsNeedPad=0{
	pos 40,320:celput 5,0,0.75,0.75,0
	pos 160,320:celput 4,0,0.75,0.75,0
	pos 460,320:celput 6,0,0.75,0.75,0
	
	IsPress4 = "F" : IsPress5 = "F" : IsPress6 = "F"
		mtlist touch_id
		touch_cnt = stat

		repeat touch_cnt
			dim t_info, 4
			mtinfo t_info, touch_id(cnt)
			if t_info(0) == 1 {
				if (t_info(1) >= area_a(0)) && (t_info(1) <= area_a(2)) && (t_info(2) >= area_a(1)) && (t_info(2) <= area_a(3)) { 
					IsPress6 = "T"
				}else{
					IsPress6 = "F"
				}

				if (t_info(1) >= area_b(0)) && (t_info(1) <= area_b(2)) && (t_info(2) >= area_b(1)) && (t_info(2) <= area_b(3)) { 
					IsPress5 = "T"
				}else{
					IsPress5 = "F"
				}

				if (t_info(1) >= area_c(0)) && (t_info(1) <= area_c(2)) && (t_info(2) >= area_c(1)) && (t_info(2) <= area_c(3)) { 
					IsPress4 = "T"
				}else{
					IsPress4 = "F"
				}
			}
		loop
}else:if IsNeedPad=1{
		mtlist tid: mtinfo p,tid: tx=ixy(p(1)): ty=ixy(p(2))
		if p { if tf { tf=2: tc++ } else { tf=1: tx1=tx: ty1=ty }}
		else { if tf { if tf!3 { tf=3 } else { tf=0: tc=0 }}}

		// 移動操作を判定
		if tf {
			x+=double((tx1<tx)-(tx1>tx)) 
			y+=double((ty1<ty)-(ty1>ty))
			if double((tx1<tx)-(tx1>tx))<0{IsPress5=0:IsPress4=1}else:if double((tx1<tx)-(tx1>tx))>0{IsPress5=1:IsPress4=0}else{IsPress5=1:IsPress4=1}
			if double((ty1<ty)-(ty1>ty))<0{IsPress6=0}else{IsPress6=1}
		}

		if tf=0{IsPress6=1:IsPress5=1:IsPress4=1}

		// ジャンプ操作の判定
		if btncnt {
			if jump=0 & btncnt=1 { jump=1: jy=y: jsp=20}
		}
		// ジャンプ中の処理
		if jump {
			y-=jsp: jsp-=(2-(btncnt>0))
			if jy<=y { jump=0: y=jy }
		}

		// 描画
		if tf=2{font "",30:color 255,0,0: locate tx1-4,ty1-4: mes "↑": locate tx1-24,ty1-4: mes "←": locate tx1+16,ty1-4: mes "→"}	// 方向操作起点
		await 16
}
	redraw 1

	if myflag>0 : goto *main2
	if coin>=coinmax-yuuyo : goto *main3

	await 16
	goto *main


*main2
	;	プレイヤーやられ
	mmstop
	mmplay 1
	redraw 0
	gosub *drawbg
	gosub *drawmap
	gosub *mymove

	font msgothic,40,1
	pos 150,200
	color 255,255,255
	mes "失敗..."
	font "",16:mes "\n\n\n放送局「死んだ回を放送はできませんね」"
	redraw 1
	await 1000
	goto *gtitle


*main3
	;	クリア
	if MapScore=8{Epi=1}
	pos 40,330:objsize 280,100:button "Twitterに共有",*twitter
	pos 320,330:objsize 280,100:button "コース一覧へ戻る",*gtitle
	repeat 40
	redraw 0
	gosub *drawbg
	gosub *drawmap
	gosub *mymove

	font msgothic,40,1
	pos 50,20
	color 255,255,255
	mes "クリア"
	pos 20,100:font "",24
	mes "コース "+MapTitle+" [World"+MapScore+"]"
	font "",16:mes "\n\n\n放送局「"+ClearText+"」"

	redraw 1
	if cnt=16 : mmstop : mmplay 0
	await 16
	loop
	stop

*twitter
	exec "https://x.com/intent/post?text=%E3%82%84%E3%81%A3%E3%81%9F%EF%BC%81%20https://AbellAte.net/Gameの「AbellApple」の"+MapTitle+" [World"+MapScore+"]"+"%E3%82%92%E3%82%AF%E3%83%AA%E3%82%A2%E3%81%97%E3%81%9F%EF%BC%81",16
	stop

*mymove
	;	プレイヤー操作
	stick key,15
	x=px:y=py
	myrun=0

	if myflag!=0 {
		mydir=32
		myflag=myflag+1
		key=0
	}
	
	if IsPress5="T" and IsNeedPad!=2 {mydir=12 : myrun=1 : x=x-myspd}
	if IsPress4="T" and IsNeedPad!=2 {mydir=28 : myrun=1 : x=x+myspd}
	
	if key&1 : mydir=12 : myrun=1 : x=x-myspd
	if key&4 : mydir=28 : myrun=1 : x=x+myspd
	if x<0 : x=0
	if x>=(vmaxx-mysizex) : x=px

	if myrun>0 {
		i=mysizey-1 : gosub *myarea
		if a>0 {
			gosub *chkitem
			x=px
		}
	}

	if myjmp>0 : goto *mymove3

	;	通常時
	
	if key&16 {IsPress6="OK"}
	if IsPress6="T" and myjmp!=1 and IsNeedPad=0 : IsPress6="OK"
	if IsPress6="OK" : myjmp=1 : mygrv=-grav : IsPress6="" :mmplay 3 : goto *mymove

	i=mysizey : gosub *myarea
	if a=0 : myjmp=1 : mygrv=8 : goto *mymove3

	goto *mymove2

*mymove3
	if water=0{myjmp=1:mygrv=-grav}
	;	ジャンプ中
	y=y+mygrv
	mygrv=mygrv+1

	if mygrv<0 : goto *mymove2
	if mygrv>16 : mygrv=16

	i=mysizey : gosub *myarea
	if a>0 {
		mmplay 2
		gosub *chkitem
		y=(mapy*sy)-mysizey : myjmp=0
		mydir=0
	}
	if y>=vmaxy : gosub *mydead

*mymove2
	;	プレイヤー描画
	px=x:py=y

	a=mydir
	if myrun>0 {
		myani=myani+1
		a+=(myani/4)\4
	}

	gmode 2,mysizex,mysizey
	x=px-viewx:y=py-viewy
	pos x,y:celput 3,a

	i=0
	if x<240 : i=(x-240)/40
	if x>(wx-420) : i=(x-(wx-420))/40
	viewx=limit( viewx+i, 0, vbasex )

	i=0
	if y<160 : i=(y-160)/40
	if y>(wy-240) : i=(y-(wy-240))/40
	viewy=limit( viewy+i, 0, vbasey )

	return

*myarea
	;	自分の足元マップを調べる
	mapx=(x+10)/sx
	mapy=(y+i)/sy
	if a=23{mygrv=-50}
	if mapy>=mapsizey : a=0 : return
	if mapy<0 : a=0 : return
	a=map(mapx,mapy)
	if a>=hitparts : return		; 接触のあるパーツID
	mapx=(x+53)/sx
	a=map(mapx,mapy)
	if a>=hitparts : return		; 接触のあるパーツID
	a=0
	return

*chkitem
	;	障害物・アイテムのチェック
	if a=3 {
		coin=coin+1
		map(mapx,mapy)=0
		mmplay 4
		return
	}
	if a<6 {
		gosub *mydead
	}
	return

*mydead
	;	やられフラグを立てる
	myflag=1
	return

*drawbg
	;	背景描画
	startx=-viewx\sx	; マップの表示開始X座標
	starty=-viewy\sy	; マップの表示開始Y座標
	ofsx=viewx/sx		; マップデータの参照開始X座標
	ofsy=viewy/sy		; マップデータの参照開始Y座標

	;	奥の背景も少しスクロールさせる
	gmode 0,wx,wy
	x=0:y=viewy*(bgmaxy-wy)/vbasey
	pos 0,0:color colors(0),colors(1),colors(2):boxf:color 0,0,0
	return

*drawmap
	;	マップ描画
	y=0
	drawx=mx:if startx!=0 : drawx=drawx+1
	drawy=my:if starty!=0 : drawy=drawy+1

	gmode 2,sx,sy
	repeat drawy
		x=0
		repeat drawx
			a=map(ofsx+x,ofsy+y)
			if a>0 : pos x*sx+startx,y*sy+starty:celput 2,a
			x=x+1
		loop
		y=y+1
	loop
	return

